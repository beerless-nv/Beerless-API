{
  "name": "Article",
  "plural": "articles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "hidden": [
    "isApproved"
  ],
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "default": null
    },
    "slug": {
      "type": "string",
      "required": true,
      "default": null
    },
    "picture": {
      "type": "string",
      "default": null
    },
    "intro": {
      "type": "string",
      "default": null
    },
    "content": {
      "type": "string",
      "default": null
    },
    "statusId": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "originalId": {
      "type": "number",
      "required": false,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "tags": {
      "type": "hasMany",
      "model": "Tag",
      "foreignKey": "articleId",
      "options": {
        "nestRemoting": true
      },
      "through": "ArticleTag"
    },
    "editors": {
      "type": "hasMany",
      "model": "UserFull",
      "foreignKey": "articleId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "activityTypes": {
      "type": "hasMany",
      "model": "ActivityType",
      "foreignKey": "articleId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "author": {
      "type": "belongsTo",
      "model": "UserFull",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
