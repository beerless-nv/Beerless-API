{
  "name": "Contact",
  "plural": "contacts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "properties": {
    "website": {
      "type": "string",
      "default": null
    },
    "twitter": {
      "type": "string",
      "default": null
    },
    "facebook": {
      "type": "string",
      "default": null
    },
    "instagram": {
      "type": "string",
      "default": null
    }
  },
  "validations": [],
  "relations": {
    "brewery": {
      "type": "hasOne",
      "model": "Brewery",
      "foreignKey": "contactId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
