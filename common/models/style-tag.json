{
  "name": "StyleTag",
  "plural": "styleTags",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "null"
    },
    "description": {
      "type": "string",
      "required": false,
      "default": "null"
    }
  },
  "validations": [],
  "relations": {
    "styleTagCategory": {
      "type": "belongsTo",
      "model": "StyleTagCategory",
      "foreignKey": "styleTagCategoryID",
      "options": {
        "nestRemoting": true
      }
    },
    "beers": {
      "type": "hasMany",
      "model": "Beer",
      "foreignKey": "styleTagId",
      "keyThrough": "beerId",
      "options": {
        "nestRemoting": true
      },
      "through": "Beerstyle"
    }
  },
  "acls": [],
  "methods": {}
}
