{
  "name": "Activity",
  "plural": "activities",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "properties": {
    "statusId": {
      "type": "number",
      "required": true,
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "beer": {
      "type": "belongsTo",
      "model": "Beer",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "UserFull",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "article": {
      "type": "belongsTo",
      "model": "Article",
      "foreignKey": "articleId",
      "options": {
        "nestRemoting": true
      }
    },
    "brewery": {
      "type": "belongsTo",
      "model": "Brewery",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      }
    },
    "activityType": {
      "type": "belongsTo",
      "model": "ActivityType",
      "foreignKey": "activityTypeId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
