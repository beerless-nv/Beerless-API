{
  "name": "Tastingprofile",
  "plural": "tastingprofiles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "properties": {
    "malty": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "sweet": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "sour": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "hoppy": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "bitter": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "fruity": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "beer": {
      "type": "belongsTo",
      "model": "Beer",
      "foreignKey": "beerId"
    },
    "userFull": {
      "type": "belongsTo",
      "model": "UserFull",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
