{
  "name": "Beer",
  "plural": "beers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "null"
    },
    "ABV": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "IBU": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "EBC": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "temperature": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "fermentation": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "glass": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "logo": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "description": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "season": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "since": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "isApproved": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "styleTags": {
      "type": "hasMany",
      "model": "StyleTag",
      "foreignKey": "beerId",
      "keyThrough": "styleTagId",
      "options": {
        "nestRemoting": true
      },
      "through": "Beerstyle"
    },
    "tastingprofiles": {
      "type": "hasMany",
      "model": "Tastingprofile",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      }
    },
    "breweries": {
      "type": "hasMany",
      "model": "Brewery",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      },
      "through": "BeerFromBrewery"
    },
    "users": {
      "type": "hasMany",
      "model": "UserFull",
      "foreignKey": "favouriteBeerId",
      "options": {
        "nestRemoting": true
      }
    },
    "editors": {
      "type": "hasMany",
      "model": "UserFull",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "activityTypes": {
      "type": "hasMany",
      "model": "ActivityType",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "beerFromBreweries": {
      "type": "hasMany",
      "model": "BeerFromBrewery",
      "foreignKey": "beerId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
