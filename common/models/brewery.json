{
  "name": "Brewery",
  "plural": "breweries",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "timestampCreated",
      "updatedAt": "timestampUpdated"
    },
    "UserRestrictions": {
      "email": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": null
    },
    "description": {
      "type": "string",
      "default": null
    },
    "country": {
      "type": "string",
      "default": null
    },
    "province": {
      "type": "string",
      "default": null
    },
    "place": {
      "type": "string",
      "default": null
    },
    "postcode": {
      "type": "string",
      "default": null
    },
    "streetAndNumber": {
      "type": "string",
      "default": null
    },
    "logo": {
      "type": "string",
      "default": "beer-no-logo.svg"
    },
    "statusId": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "originalId": {
      "type": "number",
      "required": false,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "beers": {
      "type": "hasMany",
      "model": "Beer",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      },
      "through": "BeerFromBrewery"
    },
    "contact": {
      "type": "hasOne",
      "model": "Contact",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      }
    },
    "editors": {
      "type": "hasMany",
      "model": "UserFull",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "activityTypes": {
      "type": "hasMany",
      "model": "ActivityType",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      },
      "through": "Activity"
    },
    "beerFromBreweries": {
      "type": "hasMany",
      "model": "BeerFromBrewery",
      "foreignKey": "breweryId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
